{
  "name": "carcleaning010-admin",
  "version": "1.2.0",
  "description": "Business Management Suite voor Carcleaning010",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "NODE_ENV=production pm2 start ecosystem.config.js --env production",
    "restart": "pm2 restart carcleaning010-admin",
    "stop": "pm2 stop carcleaning010-admin",
    "logs": "pm2 logs carcleaning010-admin",
    "monitor": "pm2 monit",
    "backup": "bash scripts/backup.sh",
    "deploy": "bash deploy.sh",
    "db:migrate": "psql -U $DB_USER -d $DB_NAME -f database/schema.sql",
    "db:seed": "psql -U $DB_USER -d $DB_NAME -f database/seed.sql",
    "init-db": "node scripts/init-database.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "multer": "^1.4.5-lts.1",
    "puppeteer": "^21.5.2",
    "nodemailer": "^6.9.7",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1",
    "moment": "^2.29.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}